        <label for="Call Sign">コールサイン　  　:</label><input type="text" name="callsign" id="txt-clsg" value="" placeholder="貴局コールサイン（６文字以内）" maxlength="6" style="width: 50%;" class="ui-input-text ui-body-c ui-corner-all ui-shadow-inset"><br />
        <label for="Call sign date">交信日(From〜To)　   :</label>
        <input type="text" name="fromdate" id="datepicker" value="" placeholder="書式:yyyymmdd" maxlength="8" style="width: 20%;" class="ui-input-text ui-body-c ui-corner-all ui-shadow-inset"><label for="Call sign dateT">〜</label>
        <input type="text" name="todate" id="datepicker" value="" placeholder="書式:yyyymmdd" maxlength="8" style="width: 20%;" class="ui-input-text ui-body-c ui-corner-all ui-shadow-inset"><br />
        <br />

        <input class="ui-button ui-body-c ui-corner-all" style="width: 30%; padding: 10px;"  type="button" id="btn-search" value="検索" />
        <input class="ui-button ui-body-c ui-corner-all" style="width: 30%; padding: 10px;" type="button" id="btn-clear" value="クリア" />
        <br />
        <br />
        <a class="btn btn-light btn-xl js-scroll-trigger" href="#portfolio">QSLアップロード</a>
        <br />
